{
    "name": "zgabievi/laravel-bog-console",
    "description": "Bank of Georgia Merchant Console OpenAPI Helper",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zura Gabievi",
            "email": "zura.gabievi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zorb\\BOGConsole\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/config": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "bensampo/laravel-enum": "^1.38|^3.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zorb\\BOGConsole\\BOGConsoleServiceProvider"
            ],
            "aliases": {
                "BOGConsole": "Zorb\\BOGConsole\\Facades\\BOGConsole"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
